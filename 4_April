let db;

db.heroes.find({
  powers: {
    $in: ["robot", "intelligence"],
  },
});

db.heroes.find({
  villains: {
    $elemMatch: { name: "Hela" },
  },
});

db.heroes.find({
  villains: { name: "Hela", health: 87 },
});

db.heroes.find({
  "villains.name": "Hela",
});

db.heroes.find(
  {
    health: {
      $lt: 50,
    },
  },
  { name: 1, health: 1, _id: 0 }
);
